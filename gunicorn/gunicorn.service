[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/django-react-media-pro
ExecStart=/home/ubuntu/django-react-media-pro/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
          djangomediapro.wsgi:application
[Install]
WantedBy=multi-user.target
